{
  "manifest_version": 3,
  "name": "GitHub JIRA Ticket Linker",
  "description": "",
  "version": "1.2.0",
  "content_scripts": [{
    "matches": ["*://github.com/*","*://*/*/pull*"],
    "js":      ["js/storage.js", "js/jquery-3.7.1.min.js", "js/content.js"]
    }],
  "permissions": ["storage"],
  "action": {
     "default_icon": "image/icon.png",
     "default_popup": "popup.html",
     "default_title": "GitHub JIRA Ticket Linker"
  },
  "icons": {
     "128": "image/icon.png"
  }
}
